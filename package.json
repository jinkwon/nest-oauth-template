{
  "name": "nest-oauth-template",
  "version": "0.0.1",
  "description": "",
  "author": "jinkwon lee <uzmystic@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "yarn run start:dev",
    "prebuild": "rimraf dist",
    "build": "next build && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env PHASE=dev NEXT_PHASE=local nest start -p ./tsconfig.server.json --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "8.0.6",
    "@nestjs/core": "8.0.6",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/passport": "8.0.1",
    "@nestjs/platform-express": "8.0.6",
    "@nestjs/serve-static": "2.2.2",
    "@nestjs/swagger": "5.0.9",
    "@nestjs/typeorm": "8.0.2",
    "add": "2.0.6",
    "antd": "4.16.13",
    "axios": "0.21.1",
    "class-validator": "0.13.1",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "geojson": "0.5.0",
    "helmet": "4.6.0",
    "kill-port": "1.6.1",
    "morgan": "1.10.0",
    "next": "11.1.0",
    "passport": "0.4.1",
    "passport-google-oauth20": "2.0.0",
    "passport-jwt": "4.0.0",
    "passport-kakao": "1.0.1",
    "passport-naver": "^1.0.6",
    "pg": "8.7.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-object-table-viewer": "1.0.7",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.3.0",
    "styled-components": "5.3.1",
    "swagger-ui-express": "4.1.6",
    "typeorm": "0.2.37",
    "yarn": "1.22.11"
  },
  "devDependencies": {
    "@nestjs/cli": "8.1.1",
    "@nestjs/schematics": "8.0.3",
    "@nestjs/testing": "8.0.6",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.1",
    "@types/node": "16.7.6",
    "@types/react": "17.0.19",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.29.3",
    "@typescript-eslint/parser": "4.29.3",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.1",
    "jest": "27.1.0",
    "prettier": "2.3.2",
    "supertest": "6.1.6",
    "ts-jest": "27.0.5",
    "ts-loader": "9.2.5",
    "ts-node": "10.2.1",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
